<li class="d-flex list-group-item justify-content-between align-items-center">
    <%= matchedUser.name %>
        <div class="ms-auto d-flex justify-content-between">
            <form action="/unmatch" method="post">
                <input type="hidden" name="unmatch" value="<%= matchedUser.email %>">
                <button class="btn btn-success requestBTN m-1">Unmatch</button>
            </form>
            <% let hasReviewed=false; for (let i=0; i < matchedUsers.length; i++) { if
                (matchedUsers[i].email===matchedUser.email) { let rateList=matchedUsers[i].rate; for (let j=0; j <
                rateList.length; j++) { if (rateList[j].email===sessionEmail) { hasReviewed=true; break; } } break; } }
                %>
                <% if (!hasReviewed) { %>
                    <form action="/rate/<%= matchedUser.email %>" method="get">
                        <button type="submit" class="btn btn-success requestBTN m-1">Rate</button>
                    </form>
                    <% } else { %>
                        <div></div>
                        <% } %>
                            <form action="/chat/<%= matchedUser.chatID %>" method="get">
                                <button type="submit" class="btn btn-success requestBTN m-1">Chat</button>
                            </form>
        </div>

</li>